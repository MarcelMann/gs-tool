<gswb-menubar>
	<menu id="file" name="File">
		<item name="New" command="new.graph" strokeKey="VK_N" strokeModifier="CTRL_MASK"/>
		<item name="Open" command="open.graph" strokeKey="VK_O" strokeModifier="CTRL_MASK"/>,
		<item name="Save" command="save.graph" strokeKey="VK_S" strokeModifier="CTRL_MASK"/>
		<separator/>
		<item name="Quit" command="quit" strokeKey="VK_Q" strokeModifier="CTRL_MASK"/>
	</menu>
	<menu id="edit" name="Edit">
		<menu name="Selection">
			<item name="All" command="@CLI:select all" strokeKey="VK_A" strokeModifier="CTRL_MASK"/>
			<item name="None" command="@CLI:unselect all" strokeKey="VK_A" strokeModifier="CTRL_MASK,SHIFT_MASK"/>
		</menu>
		<separator/>
		<item name="Copy" command="@CLI:copy" strokeKey="VK_C" strokeModifier="CTRL_MASK"/>
		<item name="Cut" command="@CLI:cut" strokeKey="VK_X" strokeModifier="CTRL_MASK"/>
		<item name="Paste" command="@CLI:paste" strokeKey="VK_V" strokeModifier="CTRL_MASK"/>
		<separator/>
		<item name="Options" command="options"/>
	</menu>
	<menu id="dialog" name="Dialog"></menu>
	<menu id="help" name="Help">
		<item name="Manual" command="menu.help.manual"/>
		<item name="Tutorials" command="menu.help.tutorials"/>
		<separator/>
		<item name="About" command="menu.help.about"/>
	</menu>
</gswb-menubar>
